config {
  type: "view",
  schema: "aal_demo_devday_2026",
  description: "Silver Layer: Cleaned and standardized badge data from StackOverflow"
}

-- Silver Layer (Staging): Light transformations and standardization

SELECT
    id AS badge_id,
    user_id,
    name AS badge_name,
    date AS award_timestamp
FROM
    ${ref("bigquery-public-data", "stackoverflow", "badges")}
